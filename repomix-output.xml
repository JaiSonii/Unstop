<file path="src/components/ui/Input.tsx">
import { useState } from "react";
import { EyeOffIcon, EyeIcon } from "lucide-react";

type Props = {
    icon: React.ReactNode
    label: string
    type: string
    value: string
    onChange: React.ChangeEventHandler<HTMLInputElement>
    placeholder: string
    isPassword?: boolean
}

const Input: React.FC<Props> = ({ icon, label, type, value, onChange, placeholder, isPassword = false }) => {
    const [showPassword, setShowPassword] = useState(false);

    return (
        <div className="flex items-center bg-[#F4F4F4] p-4 border-none rounded-2xl gap-4 w-full">
            <div className="w-5 h-4">

                {icon}
            </div>
            <div className="flex flex-col flex-grow">
                <label className="text-xs font-normal text-[#A0A0A0]">{label}</label>
                <div className="flex items-center">
                    <input
                        type={isPassword && !showPassword ? 'password' : type}
                        value={value}
                        onChange={onChange}
                        placeholder={placeholder}
                        className="flex-grow bg-[#F4F4F4] outline-none text-[16px] text-black font-semibold placeholder-[#A0A0A0]"
                    />
                    {isPassword && (
                        <div onClick={() => setShowPassword(!showPassword)}>
                            {showPassword ? <EyeOffIcon /> : <EyeIcon />}
                        </div>
                    )}
                </div>
            </div>
        </div>
    );
};

export default Input
</file>

